root ::= (reflection "Finally, here is the json with the type of document : " json)

reflection ::= ("I think the type of document is " string ". " isInArray)

isInArray ::= ("It is in the possible document types array. " | (
    "However, it is not in the possible document types array. Therefore, I should look in the array and I find that the closest type of document that is allowed is " string ". "))

json ::=  "{ \"Type de document\" : " string "}" 


string ::=
  "\"" (
    [^"\\\[\]] |
    "\\" (["\\/bfnrt] | "u" [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F]) # escapes
  )* "\"" 

ws ::= ([ \t\n] ws)?